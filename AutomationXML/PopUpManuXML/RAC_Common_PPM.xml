	<!--Open PropertiesOnRelation dialog-->
		<Name>PropertiesOnRelation</Name>
		<Value>Properties On Relation...</Value>
	</Variable>		
	<Variable>
	<!--Open Summary dialog in BOM Table-->
		<Name>OpenwithSummary</Name>
		<Value>Open with:Summary</Value>
	</Variable>
