	<!--Revise selected Teamcenter Object's revision-->
		<Name>Revise</Name>
		<Value>Revise...</Value>
	</Variable>
	<Variable>
	<!--Copy selected Teamcenter Object-->
		<Name>Copy</Name>
		<Value>Copy	Ctrl+C</Value>
	</Variable>
