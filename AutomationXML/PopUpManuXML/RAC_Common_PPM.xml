<Environment>
	<!--Delete all the comments once the menu is found/implemented-->
	<Variable>
	<!--Send selected Teamcenter Object to Structure Manager-->
		<Name>SendToStructureManager</Name>
		<Value>Send To:Structure Manager</Value>
	</Variable>
	<Variable>
	<!--Open Properties dialog-->
		<Name>ViewProperties</Name>
		<Value>View Properties</Value>
	</Variable>
	<Variable>
	<!--To Refresh Teamcenter Window-->
		<Name>Refresh</Name>
		<Value>Refresh	F5</Value>
	</Variable>
